#include <stdio.h>

int main() {
   char c;
   printf("enter a character:");
   scanf("%c",&c);
   if(c >= 65 && c <= 90)
      printf("Upper Case Letter");
   else if(c >= 97 && c <= 122)
      printf("Lower Case letter");
   else if(c >= 48 && c <= 57)
      printf("Number");
   else
      printf("Symbol");
    return 0;
}
